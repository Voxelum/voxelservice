{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "head"
      ]
    },
    {
      "type": "blob",
      "direction": "in",
      "name": "blobIn",
      "path": "curseforge/{Query.project}/{Query.file}",
      "dataType": "binary",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "blobOut",
      "path": "curseforge/{Query.project}/{Query.file}",
      "dataType": "binary"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "metaOut",
      "path": "curseforge/{Query.project}/{Query.file}.json",
      "dataType": "string",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "scriptFile": "../dist/get-file-by-curseforge/index.js"
}