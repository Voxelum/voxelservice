{
  "bindings": [
    {
      "name": "plSyncTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 0/1 * * *",
      "runOnStartup": true
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "mcbbsData",
      "dataType": "string",
      "path": "pure-launcher/mcbbsData.json",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "vanillaData",
      "dataType": "string",
      "path": "pure-launcher/vanillaData.json",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "scriptFile": "../dist/pl-sync-timer/index.js"
}
