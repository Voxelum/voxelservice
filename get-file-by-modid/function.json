{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "name": "metaIn",
      "direction": "in",
      "dataType": "string",
      "type": "blob",
      "connection": "AzureWebJobsStorage",
      "path": "curseforge/{Query.project}/{Query.file}"

    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "scriptFile": "./index.js"
}